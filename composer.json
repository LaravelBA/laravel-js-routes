{
    "name": "laravelba/laravel-js-routes",
    "description": "Access Laravel routes from your javascript files.",
    "license": "MIT",
    "keywords": ["laravel", "generators", "javascript", "routes"],
    "authors": [
        {
            "name": "Fede Isas",
            "email": "fedeisas@hotmail.com"
        },
        {
            "name": "Guido Contreras Woda",
            "email": "guiwoda@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.6.0",
        "illuminate/support": "^5.0",
        "illuminate/console": "^5.0",
        "illuminate/filesystem": "^5.0",
        "illuminate/routing": "^5.0"
    },
    "require-dev": {
        "illuminate/events": "^5.0",
        "phpunit/phpunit": "^5.3",
        "mockery/mockery": "^1.0.0@dev",
        "squizlabs/php_codesniffer": "^2.6",
        "jakub-onderka/php-parallel-lint": "^0.9",
        "jakub-onderka/php-console-highlighter": "^0.3",
        "phpmd/phpmd": "^2.4",
        "fabpot/php-cs-fixer": "^1.11"
